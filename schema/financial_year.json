      }
    },
    {
      "label": "End Date",
      "name": "endDate",
      "dataType": "string",
      "controlType": "text",
      "validation": {
        "required": true
      }
    },
    {
      "label": "Is Opened",
      "name": "isOpened",
      "dataType": "boolean",
      "controlType": "checkbox",
      "validation": {
        "required": true
      }
    }
  ]
